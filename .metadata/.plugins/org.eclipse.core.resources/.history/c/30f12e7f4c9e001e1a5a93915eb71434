package mine;
class Ac{
	void show() {
		System.out.println("hii");
//		return this;
	}
}
class Bc extends Ac{
void show() {
//		super.show();
		System.out.println("hlo");
//		return this;
		
	}
}

public class Covarient {
	public static void main(String[] args) {
		
	
Bc b=(Bc)new Ac();
Ac a=new Bc();
b.show();
a.show();
	}
}
